<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maceo.babylog">
    <meta-data android:name="firebase_crashlytics_collection_enabled" android:value="false" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/babylog_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BabyInfoActivity" />
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".FeedingActivity"
            android:label="Feeding" />
        <activity
            android:name=".SleepActivity"
            android:label="Sleep" />
        <activity
            android:name=".ChartActivity"
            android:label="Charts" />
        <activity
            android:name=".DiaperChangeActivity"
            android:label="Diaper Change" />
        <activity
            android:name=".NotificationActivity"
            android:label="Daily Reminders" />
        <activity android:name=".TummyTimeActivity" />
        <activity android:name=".WeighyTrackingActivity" />
        <activity
            android:name=".FeedingBottleActivity"
            android:label="Bottle" />
        <activity
            android:name=".BreastFeedingActivity"
            android:label="Breast Feeding" />
        <activity
            android:name=".NutritionActivity"
            android:label="Meal" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".EditProfileActivity"
            android:label="Edit Profile" />
        <activity
            android:name=".MemoriesActivity"
            android:label="Memories" />

        <receiver
            android:name=".AlertReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ImagesActivity"></activity>
    </application>

</manifest>